public class CadeiaCaracteres {

    public static void main(String args[]){

        String nome = "Mariana", sobrenome =  " Chasco";
        String nomeCompleto = nome + sobrenome;
        int teste = 10
        System.out.println("Nome Completo: " + nomeCompleto + teste); //
        System.out.println("Tamanho String: " + nomeCompleto.length()); //nomecompleto: objeto, string(classe) length: vai pegar o objeto e vai aponta todos complementos dele e mostra seu tamanho 
        System.out.println("Igual a Mariana Chasco: " + nomeCompleto.equals("Mariana Chasco")); //equals: compara se as string s√£o exatamentes iguais 
        System.out.println("Igual a MARIANA CHASCO:" + nomeCompleto.equalsIgnoreCase("MARIANA CHASCO"));
        }
}


